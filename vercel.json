{
    "redirects": [
      {
        "source": "/",
        "destination": "/en/index.html",
        "permanent": true
      }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600, immutable"
                }
            ]
        },
        {
            "source": "/(.*).(php)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
  }